                "html-template.ng meta.element.structure.html meta.attribute.for.html entity.other.attribute-name.html html-template.ng.attributes.generic",
                "html-template.ng meta.attribute entity.other.attribute-name.html html-template.ng.attributes.generic",
                "html-template.ng meta.element.custom.html meta.ng-binding.template.sugar.html entity.other.attribute-name.html html-template.ng.attributes",
                "html-template.ng meta.element.custom.html meta.ng-binding.event.html entity.other.attribute-name.html",
                "html-template.ng meta.ng-binding.template.sugar.html entity.other.attribute-name.html html-template.ng.attributes",
                "html-template.ng meta.element.structure.html meta.ng-binding.event.html entity.other.attribute-name.html",
                "html-template.ng meta.ng-binding.template.variable.boolean.html entity.other.attribute-name.html html-template.ng.attributes",
                "html-template.ng meta.element.structure.html meta.ng-binding.property.html entity.other.attribute-name.html",
                "html-template.ng meta.ng-binding.event.html entity.other.attribute-name.html",
                "html-template.ng meta.ng-binding.property.html entity.other.attribute-name.html entity.other.ng-binding-name.property.html",
                "html-template.ng meta.attribute.event-handler entity.other.attribute-name.html html-template.ng.attributes.events"
            ],
            "settings": {
                "foreground": "#83df55"
            }
        },
        {
            "scope": [
                "html-template.ng string.quoted.double.html",
                "html-template.ng meta.attribute.class.html string.quoted.double.html",
                "html-template.ng expression.ng html-template.ng.interpolation.expression expression.ng"
            ],
            "settings": {
                "foreground": "#fcb08ae6"
            }
        },
        {
            "scope": [
                "html-template.ng punctuation.definition.tag.begin.html",
                "html-template.ng punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#797979",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "html-template.ng meta.element.custom.html constant.language.boolean.false.ts",
                "html-template.ng meta.element.custom.html constant.language.boolean.true.ts",
                "html-template.ng keyword.control",
                "html-template.ng storage.type.ts",
                "html-template.ng keyword.operator.expression.of.ts",
                "html-template.ng constant.language.boolean",
                "html-template.ng constant.character.entity.named.nbsp.html",
