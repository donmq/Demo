    "tokenColors": [
        {
            "-------------HTML----------": "-----HTML-------"
        },
        {
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "scope": [
                "html-template.ng"
            ],
            "settings": {
                "foreground": "#cccccc"
            }
        },
        {
            "scope": [
                "entity.name.tag.html"
            ],
            "settings": {
                "foreground": "#F92672"
            }
        },
        {
            "scope": [
                "html-template.ng meta.element.inline.html meta.attribute.class.html entity.other.attribute-name.html html-template.ng.attributes.generic",
                "html-template.ng meta.attribute.class.html entity.other.attribute-name.html html-template.ng.attributes.generic",
                "html-template.ng meta.ng-binding.two-way.html entity.other.attribute-name.html entity.other.ng-binding-name.property.html",
