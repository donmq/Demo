                "html-template.ng meta.element.custom.html meta.ng-binding.event.html variable.other.property.ts"
            ],
            "settings": {
                "foreground": "#ffed91"
            }
        },
        {
            "scope": [
                "html-template.ng meta.property-list.scss meta.embeded.line.css meta.property-name.scss support.type.property-name.css",
                "html-template.ng expression.ng html-template.ng.interpolation.expression entity.name.function.pipe.ng"
            ],
            "settings": {
                "foreground": "#8AD7FF"
            }
        },
        {
            "scope": [
                "html-template.ng meta.property-list.scss meta.property-value.scss constant.numeric.css",
